{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/js/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(about|support)",
      "destination": "/$1.html"
    },
    {
      "source": "/((?!api|js|css|styles|assets|images|favicon.ico|.*\\.|about|support).*)",
      "destination": "/index.html"
    }
  ]
}